{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "no-unused-vars": "off",
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-useless-escape": "off"
  }
}
